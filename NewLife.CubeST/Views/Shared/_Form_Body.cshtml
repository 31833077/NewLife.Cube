@using NewLife;
@using XCode;
@using XCode.Configuration;
@{
    var entity = Model as IEntity;
    var fields = ViewBag.Fields as IList<FieldItem>;
}
@foreach (var item in fields)
{
    if (!item.IsIdentity)
    {
        @Html.PartialAsync("_Form_Group", new ValueTuple<IEntity, FieldItem>(entity, item)).Result
    }
}
@Html.PartialAsync("_Form_Footer", entity).Result
@Html.PartialAsync("_Form_Action", entity).Result
