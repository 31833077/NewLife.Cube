@{
    var set = NewLife.Cube.Setting.Current;
    if (String.IsNullOrEmpty(set.Skin) || String.Equals(set.Skin, "ACE", StringComparison.OrdinalIgnoreCase))
    {
        Layout = "~/Views/Shared/_Ace_Layout.cshtml";
    }
    else
    {
        Layout = "~/Views/" + set.Skin + "/_Layout.cshtml";
    }
}